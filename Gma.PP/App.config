<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
	</configSections>
	<appSettings>
		<add key="test" value="0"/>
		<add key="width" value="42"/>
		<add key="height" value="12"/>
		<add key="begintime" value="9:48:48"/>
		<add key="paipaiurl" value="https://test.alltobid.com"/>
	</appSettings>
	<log4net>
		<appender name="DebugAppender" type="log4net.Appender.RollingFileAppender">
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="DEBUG"/>
				<levelMax value="DEBUG"/>
			</filter>
			<file value="logs\debug.txt"/>
			<appendToFile value="true"/>
			<rollingStyle value="Composite"/>
			<!--日期的格式-->
			<datePattern value="yyyyMMdd"/>
			<!--每个文件最大大小-->
			<maximumFileSize value="50MB"/>
			<!--最多的文件数量-->
			<maxSizeRollBackups value="20"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger - %message%newline"/>
			</layout>
		</appender>
		<appender name="LogAppender" type="log4net.Appender.RollingFileAppender">
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="INFO"/>
				<levelMax value="INFO"/>
			</filter>
			<file value="logs\log.txt"/>
			<appendToFile value="true"/>
			<rollingStyle value="Composite"/>
			<!--日期的格式-->
			<datePattern value="yyyyMMdd"/>
			<!--每个文件最大大小-->
			<maximumFileSize value="50MB"/>
			<!--最多的文件数量-->
			<maxSizeRollBackups value="20"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger - %message%newline"/>
			</layout>
		</appender>
		<appender name="ExceptionAppender" type="log4net.Appender.RollingFileAppender">
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="WARN"/>
				<levelMax value="FATAL"/>
			</filter>
			<file value="logs\Error.txt"/>
			<rollingStyle value="Composite"/>
			<!--日期的格式-->
			<datePattern value="yyyyMMdd"/>
			<!--每个文件最大大小-->
			<maximumFileSize value="50MB"/>
			<!--最多的文件数量-->
			<maxSizeRollBackups value="20"/>
			<datePattern value="yyyyMMdd"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %message%newline"/>
			</layout>
		</appender>
		<root>
			<level value="ALL"/>
			<appender-ref ref="DebugAppender"/>
			<appender-ref ref="LogAppender"/>
			<appender-ref ref="ExceptionAppender"/>
		</root>
	</log4net>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
</configuration>